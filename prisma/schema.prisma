generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id         String   @id @default(uuid())
  externalId String   @unique
  username   String   @unique
  imageUrl   String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
